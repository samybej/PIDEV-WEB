{% extends 'back.html.twig' %}
{% block body %}

    <section class="content">
        <div class="row">
            <div class="col-md-6">
                <div class="box box-primary">
                    <div class="box-header">
                        <h3 class="box-title">Modifier véhicule</h3>
                    </div><!-- /.box-header -->
                    <!-- form start -->


                    {{ form_start(f) }}
                    <div class="box-body">
                        <div class="form-group">
                            <label for="exampleInputEmail1">Immatriculation</label>
                            {{ form_widget(f.immatriculation , { 'attr': { 'placeholder': 'Immatriculation'}}) }}
                        </div>
                        <div class="form-group">
                            <label for="exampleInputPassword1">Numéro assurance</label>
                            {{ form_widget(f.numeroAssurance , { 'attr': { 'placeholder': 'Numéro assurance'}}) }}
                        </div>
                        <div class="form-group">
                            <label for="exampleInputPassword1">Marque</label>
                            {{ form_widget(f.marque , { 'attr': { 'placeholder': 'Marque'}}) }}
                        </div>
                        <div class="form-group">
                            <label for="exampleInputPassword1">Type</label>
                            {{ form_widget(f.type , { 'attr': { 'placeholder': 'Type'}}) }}
                        </div>
                        <div class="form-group">
                            <label for="exampleInputPassword1">Etat</label>
                            {{ form_widget(f.etat , { 'attr': { 'placeholder': 'Etat'} }) }}
                        </div>


                    </div><!-- /.box-body -->

                    <div class="box-footer">
                        {{ form_widget (f.Ajouter) }}
                    </div>

                    {{ form_end(f) }}
                </div><!-- /.box -->
            </div>
        </div>
    </section>
{% endblock %}